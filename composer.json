{
    "name": "spatie/laravel-local-login",
    "description": "Quickly login to your local environment",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-local-login"
    ],
    "homepage": "https://github.com/spatie/laravel-local-login",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LocalLogin\\": "src",
            "Spatie\\LocalLogin\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LocalLogin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\LocalLogin\\LocalLoginServiceProvider"
            ],
            "aliases": {
                "LocalLogin": "Spatie\\LocalLogin\\Facades\\LocalLogin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}